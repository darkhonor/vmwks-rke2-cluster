{
  "default": true,
  "MD001": true,
  "MD003": {
    "style": "atx"
  },
  "MD004": {
    "style": "dash"
  },
  "MD007": {
    "indent": 4
  },
  "MD013": {
    "line_length": 80,
    "code_blocks": false,
    "tables": false
  },
  "MD022": {
    "lines_above": 1,
    "lines_below": 1
  },
  "MD024": {
    "allow_different_nesting": true
  },
  "MD025": {
    "front_matter_title": "^\\s*title\\s*[:=]"
  },
  "MD026": {
    "punctuation": ".,;:!。，；：！"
  },
  "MD029": {
    "style": "ordered"
  },
  "MD033": {
    "allowed_elements": ["br", "sub", "sup", "kbd", "mark", "details", "summary"]
  },
  "MD036": {
    "punctuation": ".,;:!?。，；：！？"
  },
  "MD040": true,
  "MD041": false,
  "MD046": {
    "style": "fenced"
  }
}